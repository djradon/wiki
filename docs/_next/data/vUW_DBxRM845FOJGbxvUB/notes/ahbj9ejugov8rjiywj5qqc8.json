"type":"wiki","from":{"fname":"prdct.shacl","id":"ahbj9ejugov8rjiywj5qqc8","vaultName":"public-notes"},"value":"prdct.skos","position":{"start":{"line":117,"column":517,"offset":5395},"end":{"line":117,"column":531,"offset":5409},"indent":[]},"xvault":false,"sameFile":false,"to":{"fname":"prdct.skos"}},{"type":"wiki","from":{"fname":"prdct.shacl","id":"ahbj9ejugov8rjiywj5qqc8","vaultName":"public-notes"},"value":"ar.topquadrant.why-i-dont-use-owl-anymore","position":{"start":{"line":126,"column":3,"offset":5905},"end":{"line":126,"column":48,"offset":5950},"indent":[]},"xvault":false,"sameFile":false,"to":{"fname":"ar.topquadrant.why-i-dont-use-owl-anymore"}},{"type":"wiki","from":{"fname":"prdct.shacl","id":"ahbj9ejugov8rjiywj5qqc8","vaultName":"public-notes"},"value":"ar.topquadrant.why-i-use-shacl-for-defining-ontology-models","position":{"start":{"line":130,"column":3,"offset":6194},"end":{"line":130,"column":66,"offset":6257},"indent":[]},"xvault":false,"sameFile":false,"to":{"fname":"ar.topquadrant.why-i-use-shacl-for-defining-ontology-models"}},{"type":"wiki","from":{"fname":"prdct.shacl","id":"ahbj9ejugov8rjiywj5qqc8","vaultName":"public-notes"},"value":"ar.hackernews.semantic-web-is-like-the-guy-that-tells-everyone-that-he-is-an-asshole","position":{"start":{"line":131,"column":3,"offset":6260},"end":{"line":131,"column":91,"offset":6348},"indent":[]},"xvault":false,"sameFile":false,"to":{"fname":"ar.hackernews.semantic-web-is-like-the-guy-that-tells-everyone-that-he-is-an-asshole"}},{"type":"wiki","from":{"fname":"prdct.shacl","id":"ahbj9ejugov8rjiywj5qqc8","vaultName":"public-notes"},"value":"ar.a-formal-approach-for-customization-of-schema-org-based-on-shacl","position":{"start":{"line":132,"column":3,"offset":6351},"end":{"line":132,"column":74,"offset":6422},"indent":[]},"xvault":false,"sameFile":false,"to":{"fname":"ar.a-formal-approach-for-customization-of-schema-org-based-on-shacl"}},{"type":"wiki","from":{"fname":"prdct.shacl","id":"ahbj9ejugov8rji